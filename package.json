{
  "name": "pidtree",
  "version": "0.1.0",
  "description": "Cross platform children list of a PID",
  "license": "MIT",
  "homepage": "http://github.com/simonpri/pidtree#readme",
  "repository": "github:simonpri/pidtree",
  "bugs": {
    "url": "https://github.com/simonpri/pidtree/issues",
    "email": "simonepri@outlook.com"
  },
  "author": "Simone Primarosa <simonepri@outlook.com> (https://github.com/simonepri)",
  "contributors": [
    "Simone Primarosa <simonepri@outlook.com> (https://github.com/simonepri)"
  ],
  "keyboards": [
    "ps-tree",
    "ps",
    "tree",
    "ppid",
    "pid",
    "pidtree"
  ],
  "main": "index.js",
  "bin": {
    "pidtree": "./bin/pidtree.js"
  },
  "files": [
    "bin",
    "index.js"
  ],
  "engines": {
    "node": ">=0.10"
  },
  "scripts": {
    "update": "npm-check -u",
    "release": "np",
    "lint": "xo",
    "test": "npm run lint&& nyc ava -m \"!*benchmark*\"",
    "bench": "ava -m \"*benchmark*\""
  },
  "devDependencies": {
    "ava": "*",
    "mockery": "^2.1.0",
    "np": "*",
    "npm-check": "*",
    "nyc": "*",
    "pify": "^3.0.0",
    "string-to-stream": "^1.1.0",
    "through": "^2.3.8",
    "time-span": "^2.0.0",
    "tree-kill": "^1.1.0",
    "xo": "*"
  },
  "ava": {
    "verbose": true
  },
  "nyc": {
    "reporter": [
      "lcovonly",
      "text"
    ]
  },
  "xo": {
    "prettier": true,
    "space": true,
    "rules": {
      "prefer-arrow-callback": 0,
      "no-var": 0
    }
  }
}
